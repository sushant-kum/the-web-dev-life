{
  "name": "the-web-dev-life",
  "version": "0.0.1",
  "description": "Source code for articles and demos presented in blog and instagram page thewebdevlife",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sushant-kum/the-web-dev-life.git"
  },
  "author": "Sushant Kumar <sushant.kum96@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sushant-kum/the-web-dev-life/issues"
  },
  "homepage": "https://github.com/sushant-kum/the-web-dev-life#readme",
  "scripts": {
    "prepare": "husky install",
    "start": "npm run serve::app",
    "ng": "ng",
    "build": "npm run build:prod::app",
    "test": "ng test",
    "preserve::app": "npm run tool:version-info-gen -- --dev",
    "serve::app": "ng serve --port 5020",
    "prebuild:prod::app": "npm run tool:version-info-gen",
    "build:prod::app": "ng build",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier *.{css,html,js,json,md,scss,ts}",
    "format:check": "prettier --check ./**/*.{css,html,js,json,md,scss,ts}",
    "format:fix": "prettier --write ./**/*.{css,html,js,json,md,scss,ts}",
    "tool:commit": "cz",
    "tool:release": "standard-version",
    "tool:version-info-gen": "node tools/version-info-gen/version-info-gen.tool.ts"
  },
  "dependencies": {
    "@angular/animations": "~13.3.10",
    "@angular/cdk": "^13.3.8",
    "@angular/common": "~13.3.10",
    "@angular/compiler": "~13.3.10",
    "@angular/core": "~13.3.10",
    "@angular/forms": "~13.3.10",
    "@angular/material": "^13.3.8",
    "@angular/platform-browser": "~13.3.10",
    "@angular/platform-browser-dynamic": "~13.3.10",
    "@angular/router": "~13.3.10",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.7",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "~13.3.7",
    "@angular/compiler-cli": "~13.3.10",
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@commitlint/cz-commitlint": "^17.0.0",
    "@types/jasmine": "^4.0.3",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "chalk": "^4.1.2",
    "commander": "^9.2.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fs-extra": "^10.0.0",
    "husky": "^8.0.1",
    "jasmine-core": "^4.1.1",
    "karma": "^6.3.6",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^12.4.2",
    "ng-lint-staged": "^12.0.3",
    "prettier": "^2.4.1",
    "prettier-eslint": "^15.0.0",
    "pretty-quick": "^3.1.1",
    "standard-version": "^9.3.2",
    "typescript": "~4.6.4"
  }
}
